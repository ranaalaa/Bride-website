<h1>Stores</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @suits.each do |suit| %>
    
        <%= image_tag suit.photo.url %><br>
        <%= link_to suit.name, suit %>
        <%= link_to 'Edit', edit_suit_path(suit) %>
    
    
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store', new_suit_path %>
