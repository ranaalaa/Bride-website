<h1>Spas & Beauty centers</h1>

<table>
  <thead>
    <tr>
     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <%= image_tag post.photo.url(:medium) %>
         <%= link_to post.name, post  %>
     &nbsp; <%= link_to 'Edit', edit_post_path(post) %>
      <br>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store', new_post_path %>
